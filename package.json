{
  "name": "xlab_web",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "npx next dev -p 3000",
    "dev:clean": "node scripts/optimize.js && npm run dev",
    "dev:silent": "cross-env NODE_OPTIONS=--no-warnings npx next dev",
    "prebuild": "node scripts/optimize.js --pre-build",
    "build": "npx next build && node scripts/post-build.js",
    "build:export": "cross-env NEXT_PUBLIC_EXPORT=true npx next build && node scripts/post-build.js",
    "build:clean": "npx rimraf .next node_modules/.cache && npm run build",
    "start": "cross-env NODE_ENV=production node .next/standalone/server.js",
    "start:prod": "cross-env NODE_ENV=production node scripts/direct-start.js",
    "lint": "npx next lint",
    "lint:fix": "npx next lint --fix",
    "format": "prettier --write .",
    "clean": "rimraf .next node_modules/.cache && npm cache clean --force",
    "optimize": "node scripts/optimize.js",
    "type-check": "tsc --noEmit",
    "analyze": "cross-env ANALYZE=true next build",
    "test": "jest",
    "test:watch": "jest --watch",
    "check": "npm run lint && npm run type-check"
  },
