{
  "name": "xlab_web",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "predev": "node fixall.js && echo Preparing development environment...",
    "dev": "next dev",
    "dev:silent": "node fixall.js && cross-env NEXT_IGNORE_WARNINGS=NEXT_PACKAGE_WARNING,ESLINT_WARNING,API_ROUTE_IMPORT_WARNING NODE_OPTIONS=--no-warnings next dev",
    "dev:win": "node fixall.js && set NODE_OPTIONS=--no-warnings && next dev",
    "dev:wasm": "cross-env NEXT_DISABLE_SWC_NATIVE=1 NEXT_USE_SWC_WASM=1 NODE_OPTIONS=--no-warnings next dev",
    "dev:no-hot": "cross-env NODE_OPTIONS='--no-warnings' next dev --no-hot",
    "dev:clean": "node fixall.js && next dev",
    "dev:export": "cross-env NEXT_PUBLIC_EXPORT=true next dev",
    "dev:safe": "cross-env NODE_OPTIONS='--no-warnings --max-old-space-size=4096' next dev",
    "dev:no-trace": "cross-env NODE_OPTIONS='--no-warnings' next dev --no-trace",
    "dev:fullfix": "node fixall.js && cross-env NEXT_DISABLE_FILE_SYSTEM_TRACE=1 NODE_OPTIONS='--no-warnings' next dev",
    "prebuild": "node fixall.js && rimraf .next && echo Preparing build environment...",
    "build": "next build",
    "build:export": "cross-env NEXT_PUBLIC_EXPORT=true next build",
    "analyze": "cross-env ANALYZE=true next build",
    "start": "next start",
    "start:export": "serve out -p 3000",
    "lint": "next lint",
    "fix-all": "node fixall.js",
    "clean": "rm -rf .next"
  },
  "dependencies": {
    "@next/bundle-analyzer": "^15.2.4",
    "@next/swc-wasm-nodejs": "15.3.2",
    "@radix-ui/react-slot": "1.1.2",
    "@types/uuid": "10.0.0",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "next": "15.2.4",
    "next-auth": "4.24.10",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwind-merge": "2.2.0",
    "uuid": "11.1.0"
  },
  "devDependencies": {
    "@next/swc-win32-x64-msvc": "15.2.4",
    "@tailwindcss/forms": "0.5.7",
    "@tailwindcss/typography": "0.5.10",
    "@types/node": "20.10.5",
    "@types/react": "18.2.45",
    "@types/react-dom": "18.2.18",
    "autoprefixer": "10.4.16",
    "critters": "0.0.23",
    "cross-env": "7.0.3",
    "eslint": "9.0.0",
    "eslint-config-next": "15.2.4",
    "ignore-loader": "^0.1.2",
    "postcss": "8.4.32",
    "rimraf": "5.0.5",
    "sharp": "0.33.5",
    "tailwindcss": "3.4.0",
    "typescript": "5.3.3",
    "webpack": "5.92.0",
    "webpack-manifest-plugin": "5.0.1"
  },
  "resolutions": {
    "webpack": "5.92.0",
    "@babel/core": "7.23.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "glob": "9.3.5",
    "inflight": "2.0.0",
    "@humanwhocodes/config-array": "@eslint/config-array"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": ""
}