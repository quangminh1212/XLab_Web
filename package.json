{
  "name": "xlab_web",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "node scripts/setup-dev.js",
    "dev:simple": "next dev",
    "dev:clean": "rimraf .next && next dev",
    "dev:silent": "cross-env NODE_OPTIONS=--no-warnings next dev",
    "build": "next build",
    "build:clean": "rimraf .next && next build",
    "start": "next start",
    "lint": "next lint --fix",
    "lint:check": "next lint",
    "type-check": "tsc --noEmit",
    "clean": "rimraf .next node_modules/.cache && npm cache clean --force",
    "check": "npm run type-check && npm run lint:check",
    "fix": "npm run lint",
    "update-purchases": "node scripts/update-purchases.js",
    "verify-products": "node scripts/verify-products.js",
    "update-product-ids": "node scripts/update-product-ids.js"
  },
  "dependencies": {
    "@radix-ui/react-slot": "1.2.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cssnano": "7.0.7",
    "framer-motion": "^12.12.1",
    "json5": "2.2.3",
    "next": "15.2.4",
    "next-auth": "^4.24.10",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "^5.5.0",
    "tailwind-merge": "^2.2.0",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@next/swc-win32-x64-msvc": "15.2.4",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@types/uuid": "^10.0.0",
    "autoprefixer": "^10.4.16",
    "cross-env": "^7.0.3",
    "eslint": "^9.0.0",
    "eslint-config-next": "15.2.4",
    "postcss": "^8.4.32",
    "rimraf": "^5.0.5",
    "sharp": "^0.33.5",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "nextjs",
    "react",
    "ecommerce",
    "web-app"
  ],
