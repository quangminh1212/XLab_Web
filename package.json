{
  "name": "xlab_web",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000",
    "dev:log": "node scripts/start-with-logger.js dev -p 3000",
    "dev:clean": "node scripts/fix-next-errors.js && npm run dev",
    "dev:safe": "node scripts/fix-next-errors.js && npm run dev",
    "dev:silent": "cross-env NODE_OPTIONS=--no-warnings next dev",
    "vercel-build": "npm install --save-dev typescript @types/react && chmod +x ./node_modules/.bin/next && next build",
    "build": "next build",
    "build:export": "cross-env NEXT_PUBLIC_EXPORT=true next build",
    "start": "next start",
    "start:log": "node scripts/start-with-logger.js start",
    "start:export": "serve out -p 3000",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "clean": "rimraf .next node_modules/.cache && npm cache clean --force",
    "fix": "node scripts/fix-next-errors.js",
    "fix:security": "node scripts/fix-security-issues.js",
    "update-purchases": "node src/scripts/update-purchases.js",
    "verify-products": "node src/scripts/verify-products.js",
    "fix:all": "node scripts/fix-all-issues.js",
    "type-check": "tsc --noEmit",
    "analyze": "cross-env ANALYZE=true next build",
    "test": "jest",
    "test:watch": "jest --watch",
    "check": "npm run lint && npm run type-check"
  },
