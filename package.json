{
  "name": "xlab_web",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "predev": "node check-critters.js && echo Preparing development environment... && node fix-all-errors.js",
    "dev": "rimraf .next/cache && node fix-all-errors.js && next dev",
    "dev:silent": "cross-env NEXT_IGNORE_WARNINGS=NEXT_PACKAGE_WARNING,ESLINT_WARNING,API_ROUTE_IMPORT_WARNING NODE_OPTIONS=--no-warnings next dev",
    "dev:win": "rimraf .next && set NODE_OPTIONS=--no-warnings && next dev",
    "dev:wasm": "cross-env NEXT_DISABLE_SWC_NATIVE=1 NEXT_USE_SWC_WASM=1 NODE_OPTIONS=--no-warnings next dev",
    "dev:no-hot": "cross-env NODE_OPTIONS='--no-warnings' next dev --no-hot",
    "dev:clean": "rimraf .next && next dev",
    "dev:export": "cross-env NEXT_PUBLIC_EXPORT=true next dev",
    "dev:safe": "cross-env NODE_OPTIONS='--no-warnings' next dev",
    "dev:no-trace": "cross-env NODE_OPTIONS='--no-warnings' next dev",
    "prebuild": "rimraf .next && echo Preparing build environment...",
    "build": "node fix-all-errors.js && rimraf .next/cache && next build",
    "build:export": "cross-env NEXT_PUBLIC_EXPORT=true next build",
    "start": "next start",
    "start:export": "serve out -p 3000",
    "lint": "next lint",
    "clean": "rimraf .next node_modules/.cache && npm cache clean --force",
    "fix-errors": "node fix-all-errors.js",
    "update-purchases": "node src/scripts/update-purchases.js",
    "verify-products": "node src/scripts/verify-products.js",
    "check-products": "powershell -ExecutionPolicy Bypass -File check-products.ps1"
  },
  "dependencies": {
    "@next/swc-wasm-nodejs": "15.3.2",
    "@radix-ui/react-slot": "1.1.2",
    "@tinymce/tinymce-react": "6.1.0",
    "@types/uuid": "10.0.0",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "cssnano": "7.0.7",
    "draftjs-to-html": "0.9.1",
    "framer-motion": "12.12.1",
    "html-to-draftjs": "1.5.0",
    "json5": "2.2.3",
    "next": "15.2.4",
    "next-auth": "4.24.10",
    "node-fetch": "3.3.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-icons": "5.5.0",
    "react-quill": "2.0.0",
    "tailwind-merge": "2.2.0",
    "uuid": "11.1.0"
  },
  "devDependencies": {
    "@next/swc-win32-x64-msvc": "15.2.4",
    "@tailwindcss/forms": "0.5.7",
    "@tailwindcss/typography": "0.5.10",
    "@types/draft-js": "0.11.18",
    "@types/node": "20.10.5",
    "@types/react": "18.2.45",
    "@types/react-dom": "18.2.18",
    "@types/react-draft-wysiwyg": "1.13.8",
    "autoprefixer": "10.4.16",
    "critters": "0.0.23",
    "cross-env": "7.0.3",
    "eslint": "9.0.0",
    "eslint-config-next": "15.2.4",
    "postcss": "8.4.32",
    "rimraf": "5.0.5",
    "sharp": "0.33.5",
    "tailwindcss": "3.4.0",
    "typescript": "5.3.3",
    "webpack": "5.92.0",
    "webpack-manifest-plugin": "5.0.1"
  },
  "resolutions": {
