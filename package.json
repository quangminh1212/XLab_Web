{
  "name": "xlab_web",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "predev": "node fix-all-errors.js && echo Preparing development environment...",
    "dev": "cross-env NODE_OPTIONS=\"--no-warnings\" next dev",
    "dev:no-swc": "cross-env NODE_OPTIONS=\"--disable-swc --no-warnings\" next dev --turbo=false",
    "dev:no-hot": "cross-env NODE_OPTIONS=\"--no-warnings\" next dev --no-hot",
    "dev:clean": "node fix-all-errors.js && cross-env NODE_OPTIONS=\"--no-warnings\" next dev",
    "dev:export": "cross-env NEXT_PUBLIC_EXPORT=true NODE_OPTIONS=\"--no-warnings\" next dev",
    "dev:safe": "cross-env NODE_OPTIONS=\"--no-warnings\" next dev --turbo=false",
    "dev:no-trace": "cross-env NODE_OPTIONS=\"--no-warnings\" next dev",
    "fix-swc": "node fix-swc.js",
    "fix-trace": "node fix-trace.js",
    "prebuild": "node fix-all-errors.js && rimraf .next && echo Preparing build environment...",
    "build": "next build",
    "build:export": "NEXT_PUBLIC_EXPORT=true next build",
    "start": "next start",
    "start:export": "serve out -p 3000",
    "lint": "next lint",
    "clean": "rimraf .next node_modules/.cache && npm cache clean --force",
    "fix-errors": "node fix-all-errors.js"
  },
  "dependencies": {
    "@radix-ui/react-slot": "1.1.2",
    "@types/uuid": "10.0.0",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "cssnano": "7.0.7",
    "next": "15.2.4",
    "next-auth": "4.24.10",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwind-merge": "2.2.0",
    "uuid": "11.1.0"
  },
  "devDependencies": {
    "@next/swc-win32-x64-msvc": "15.3.2",
    "@tailwindcss/forms": "0.5.7",
    "@tailwindcss/typography": "0.5.10",
    "@types/node": "20.10.5",
    "@types/react": "18.2.45",
    "@types/react-dom": "18.2.18",
    "autoprefixer": "10.4.16",
    "critters": "0.0.23",
    "cross-env": "7.0.3",
    "eslint": "9.0.0",
    "eslint-config-next": "15.2.4",
    "postcss": "8.4.32",
    "rimraf": "5.0.5",
    "sharp": "0.33.5",
    "tailwindcss": "3.4.0",
    "typescript": "5.3.3",
