{
  "name": "xlab_web",
  "version": "1.0.0",
  "description": "XLab - Trang web bán phần mềm chất lượng cao",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "clear-next": "if exist .next rmdir /s /q .next",
    "clean-install": "npm cache clean --force && npm install --force",
    "debug": "next dev --inspect",
    "fix-next": "node fix-next.js",
    "analyze": "cross-env ANALYZE=true next build",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\""
  },
  "keywords": [
    "software",
    "ecommerce",
    "nextjs",
    "react"
  ],
  "author": "XLab Team",
  "license": "ISC",
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@svgr/webpack": "^8.1.0",
    "@types/node": "20.11.17",
    "@types/react": "18.2.55",
    "@types/react-dom": "18.2.19",
    "next": "^15.2.4",
    "next-auth": "^4.24.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "styled-jsx": "^5.1.6",
    "typescript": "5.3.3"
  },
  "resolutions": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^15.2.4",
    "cross-env": "^7.0.3",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "svgo": "^3.3.2"
  }
}