{
  "name": "xlab_web",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3000",
    "dev:log": "node scripts/start-with-logger.js dev -p 3000",
    "dev:clean": "node scripts/fix-next-errors.js && npm run dev",
    "dev:safe": "node scripts/fix-next-errors.js && npm run dev",
    "dev:silent": "cross-env NODE_OPTIONS=--no-warnings next dev",
    "build": "node ./node_modules/next/dist/bin/next build",
    "build:export": "cross-env NEXT_PUBLIC_EXPORT=true next build",
    "start": "next start",
    "start:log": "node scripts/start-with-logger.js start",
    "start:export": "serve out -p 3000",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "clean": "rimraf .next node_modules/.cache && npm cache clean --force",
    "fix": "node scripts/fix-next-errors.js",
    "fix:security": "node scripts/fix-security-issues.js",
    "update-purchases": "node src/scripts/update-purchases.js",
    "verify-products": "node src/scripts/verify-products.js",
    "fix:all": "node scripts/fix-all-issues.js",
    "type-check": "tsc --noEmit",
    "analyze": "cross-env ANALYZE=true node ./node_modules/next/dist/bin/next build",
    "check": "npm run lint && npm run type-check && npm run test -- --runInBand",
    "prepare": "node scripts/fix-next-errors.js",
    "postinstall": "node scripts/fix-next-errors.js || true",
    "health-check": "node scripts/comprehensive-health-check.js",
    "health-improve": "node scripts/gentle-improvements.js",
    "build:log": "node scripts/build-with-logger.js",
    "vercel-build": "node ./node_modules/next/dist/bin/next build",
    "logs:gc": "node scripts/logs-gc.js",
    "audit": "npm audit --audit-level=high",
    "test": "jest -c jest.config.ts",
    "test:watch": "jest -c jest.config.ts --watch"
  },
  "dependencies": {
    "@next/swc-wasm-nodejs": "15.4.6",
