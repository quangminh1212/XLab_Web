{
  "name": "xlab_web",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "node patch-next-auth.js && cross-env NODE_OPTIONS=--require=./jsx-runtime-loader.js next dev",
    "dev:debug": "node patch-next-auth.js && cross-env NODE_OPTIONS=--require=./jsx-runtime-loader.js --trace-warnings --inspect next dev",
    "build": "node patch-next-auth.js && cross-env NODE_OPTIONS=--require=./jsx-runtime-loader.js next build",
    "start": "cross-env NODE_OPTIONS=--require=./jsx-runtime-loader.js next start",
    "lint": "next lint",
    "build:debug": "node patch-next-auth.js && cross-env NODE_OPTIONS=--require=./jsx-runtime-loader.js --trace-warnings next build",
    "update-next": "npm install next@15.2.4 react@18.2.0 react-dom@18.2.0 next-auth@4.24.5 --save --force",
    "patch": "node patch-next-auth.js",
    "clear-next": "if exist .next rmdir /s /q .next"
  },
  "dependencies": {
    "@swc/helpers": "^0.5.7",
    "clsx": "^2.1.0",
    "cross-env": "^7.0.3",
    "next": "15.2.4",
    "next-auth": "^4.24.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwind-merge": "^2.2.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@types/node": "^20.11.25",
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "autoprefixer": "^10.4.18",
    "eslint": "^8.57.0",
    "eslint-config-next": "15.2.4",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.4.2"
  },
